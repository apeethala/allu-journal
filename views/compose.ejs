<%- include("partials/header") %> 
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label>Title</label>
        <input type="text" name="postTitle" placeholder="Title" class="form-control" >
    </div>
    <div class="form-group">
        <label >Post</label>
        <textarea name="postBody"  cols="30" rows="10" class="form-control"></textarea>
    </div>
    <div class="form-group">
            <button type="submit" name="button" class="btn btn-primary">publish</button>
    </div>
 
</form>
